<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dungeonViewer.DungeonViewerController">
   <ImageView fx:id="imgView" fitHeight="720.0" fitWidth="1280.0" focusTraversable="true" pickOnBounds="true" preserveRatio="true" />
   <Button fx:id="btnSave" mnemonicParsing="false" text="save">
      <GridPane.margin>
         <Insets left="10.0" />
      </GridPane.margin>
   </Button>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
