<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dungeonViewer.DungeonViewerController">
   <center>
      <GridPane alignment="center" hgap="10" vgap="10">
         <ImageView fx:id="imgView" fitHeight="720.0" fitWidth="1280.0" focusTraversable="true" pickOnBounds="true" preserveRatio="true" />
         <columnConstraints>
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
      </GridPane>
   </center>
   <bottom>
      <VBox prefWidth="100.0" BorderPane.alignment="CENTER">
         <Button fx:id="btnSave" mnemonicParsing="false" text="save">
            <VBox.margin>
               <Insets bottom="5.0" left="10.0" top="10.0"/>
            </VBox.margin>
         </Button>
         <Label fx:id="lblMousePos" text="Label">
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" top="5.0"/>
            </VBox.margin>
         </Label>
      </VBox>
   </bottom>
</BorderPane>
